{
  "name": "@caia/tool-app-platform",
  "version": "1.0.0",
  "description": "Application Development Platform - Orchestrating intelligent agents for rapid software development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/prakashgbid/adp-agents-platform.git"
  },
  "author": "ADP Team <team@adp.ai>",
  "license": "MIT",
  "keywords": [
    "agents",
    "development",
    "platform",
    "automation",
    "typescript",
    "sats",
    "orchestration",
    "ai"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {

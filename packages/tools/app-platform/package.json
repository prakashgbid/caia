    "pg": "^8.13.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"

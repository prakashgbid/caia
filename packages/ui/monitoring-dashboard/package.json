{
  "name": "@caia/ui-monitoring-dashboard",
  "version": "1.0.0",
  "description": "Real-time monitoring and management dashboard for CAIA agent ecosystem",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t caia-dashboard .",
    "docker:run": "docker run -p 3000:80 caia-dashboard"
  },
  "dependencies": {
    "@caia/core": "^1.0.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-virtual": "^3.0.0",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-redux": "^8.1.3",
    "reactflow": "^11.10.1",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.3.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "postcss": "^8.4.32",
    "storybook": "^7.6.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "caia",
    "ai-agents",
    "monitoring",
    "dashboard",
    "real-time",
    "react",
    "typescript",
    "websockets",
    "performance",
    "debugging"
